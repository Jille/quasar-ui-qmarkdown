{
  "name": "@quasar/quasar-ui-qmarkdown",
  "version": "1.0.1",
  "author": "Jeff Galbraith <jeff@quasar.dev>",
  "description": "Display inline markdown in your Quasar App",
  "license": "MIT",
  "module": "dist/index.esm.js",
  "main": "dist/index.common.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "lint-fix": "eslint --ext .js,.vue src --fix",
    "dev": "cd dev && yarn dev && cd ..",
    "dev:umd": "yarn build && node build/script.open-umd.js",
    "dev:ssr": "cd dev && yarn 'dev:ssr' && cd ..",
    "dev:ios": "cd dev && yarn 'dev:ios' && cd ..",
    "dev:android": "cd dev && yarn 'dev:android' && cd ..",
    "dev:electron": "cd dev && yarn 'dev:electron' && cd ..",
    "build": "node build/index.js",
    "build:js": "node build/script.javascript.js",
    "build:css": "node build/script.css.js",
    "build:api": "node build/build.api.js"
  },
  "keywords": [
    "quasar",
    "markdown"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasarframework/quasar-ui-qmarkdown.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/quasarframework/quasar-ui-qmarkdown/issues",
  "homepage": "https://github.com/quasarframework/quasar-ui-qmarkdown",
  "dependencies": {
    "markdown-it": "^10.0.0",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-imsize": "^2.0.1",
    "markdown-it-ins": "^3.0.0",
    "markdown-it-katex": "^2.0.3",
    "markdown-it-mark": "^3.0.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "markdown-it-task-lists": "^2.1.1",
    "markdown-it-toc-and-anchor": "^4.2.0",
    "prismjs": "^1.17.1"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.20.0",
    "@rollup/plugin-json": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "chalk": "^3.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-vue": "^6.0.1",
    "fs-extra": "^8.1.0",
    "node-sass": "^4.13.0",
    "open": "^7.0.0",
    "postcss": "^7.0.23",
    "postcss-rtl": "^1.5.0",
    "quasar": "^1.4.5",
    "quasar-json-api": "^0.0.10",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-es": "^3.3.9",
    "zlib": "^1.0.5"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
